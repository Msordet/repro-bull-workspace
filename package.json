{
    "name": "repro-bull-workspace",
    "version": "1.0.0",
    "private": true,
    "license": "MIT",
    "workspaces": {
      "packages": [
        "apps/backend"
      ]
    },
    "scripts": {
      "build:backend": "yarn workspace repro-bull-workspace build",
      "dev:backend": "yarn --cwd apps/backend dev",
      "nuke": "rm -fr node_modules && rm -fr ./*/*/node_modules && rm -rf ./*/*/dist && yarn"
    },
    "engines": {
      "node": "14.x"
    }
  }
  